{
  "version": 2,
  "name": "free-library-hub",
  "builds": [
    {
      "src": "index.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "/index.mjs"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "index.mjs": {
      "maxDuration": 30
    }
  },
  "regions": ["bom1"],
  "outputDirectory": "public"
}